@model part3.Models.Shop

@{
    ViewData["Title"] = "PC Express Shop";
}

<div class="input-field">
    <i class="material-icons prefix">search</i>
    <input id="icon_prefix" type="text" class="validate">
    <label for="icon_prefix">Search Product</label>
</div>
<div>
    <ul class="tabs">
        <li class="tab col s3"><a class="teal-text" href="#prebuilt">Prebuilt Configurations</a></li>
        <li class="tab col s3"><a class="teal-text" href="#hardware">Computer Hardware</a></li>
        <li class="tab col s3"><a class="teal-text" href="#peripherals">Peripherals</a></li>
        <li class="tab col s3"><a class="teal-text" href="#software">Software</a></li>
    </ul>
</div>
<hr>
<div id="prebuilt" class="col s12">
    @foreach (var item in Model.getPrebuiltConfigurations())
    {
        <p>@item.getName()</p>
    }
</div>
<div id="hardware" class="col s12">
    @foreach (var item in Model.getHardware())
    {
        <p>@item.getName()</p>
    }
</div>
<div id="peripherals" class="col s12">Test 4</div>